{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "create-block/rrze-jobs",
    "version": "3.10.3",
    "title": "Jobs RRZE",
    "description": "A block to show job offers.",
    "category": "widgets",
    "icon": "admin-users",
    "keywords": [
        "job",
        "jobs"
    ],
    "textdomain": "rrze-jobs",
    "editorScript": "file:./index.js",
    "render": "file:./render.php",
    "supports": {
        "align": true
    },
    "example": {},
    "attributes": {
        "provider": {
            "type": "string",
            "default": "interamt"
        },
        "orgids": {
            "type": "string",
            "default": ""
        },
        "jobid": {
            "type": "number",
            "default": 0
        },
        "limit": {
            "type": "number",
            "default": 0
        },
        "orderby": {
            "type": "string",
            "default": "title"
        },
        "order": {
            "type": "string",
            "default": "DESC"
        },
        "fallback_apply": {
            "type": "string",
            "default": ""
        },
        "default_subject": {
            "type": "string",
            "default": ""
        },
        "link_only": {
            "type": "boolean",
            "default": false
        },
        "category": {
            "type": "string",
            "default": ""
        },
        "fauorg": {
            "type": "string",
            "default": ""
        }
    }
}